// test: relation, bool return value
PROGRAM example(input, output, error);
BEGIN
    writelnI(0 + 2);
    writelnI(0 - 2);
    writelnI(0 * 2);
    writelnI(0 / 2);

    writelnR(0.0 + 2.0);
    writelnR(0.0 - 2.0);
    writelnR(0.0 * 2.0);
    writelnR(0.0 / 2.0);

    writelnI(0 > 2);
    writelnI(0 < 2);
    writelnI(0 = 2);
    writelnI(0 <= 2);
    writelnI(0 >= 2);
    writelnI(0 != 2);
    writelnI(0.0 > 2.0);
    writelnI(0.0 < 2.0);
    writelnI(0.0 = 2.0);
    writelnI(0.0 <= 2.0);
    writelnI(0.0 >= 2.0);
    writelnI(0.0 != 2.0);
END.
